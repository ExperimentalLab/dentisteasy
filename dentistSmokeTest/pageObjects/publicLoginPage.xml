<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:ate="http://www.bigtester.org/schema/ate" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd 
	http://www.springframework.org/schema/aop 
	http://www.springframework.org/schema/aop/spring-aop-4.0.xsd
	http://www.bigtester.org/schema/ate http://www.bigtester.org/schema/ate/ate.xsd"
	>
	<!-- this registers beans annotated with @Aspect -->
	<aop:aspectj-autoproxy/>

	<import resource="classpath:META-INF/logger-reporter.xml" />
	<import resource="classpath:META-INF/page_base_bean.xml" />

	<ate:regularPage id="publicLoginPage" parent="myBasePageObject" dataFile="dentistSmokeTest/pageObjects/publicLoginPage.data.xml"/>

	<ate:pagePropertyCorrectnessAsserter
		id="publicLoginPagePropertyAsserter" resultPage="publicLoginPage" stepERValue="publicLoginPageValidationValue"/>

	<ate:pageElementExistenceAsserter
		id="publicLoginPageElementExistenceAsserter" resultPage="publicLoginPage" stepERValue="publicLoginPageValidationValue" />
		

	<ate:stepExpectedResultValue id="publicLoginPageValidationValue"
		dataValueID="publicLoginPageVID" parent="baseERValue" />

	<ate:pageElementExistenceAsserter
		id="publicLoginWrongPasswdElementExistenceAsserter" resultPage="publicLoginPage" stepERValue="publicLoginWrongPasswdValidationValue" />

	<ate:stepExpectedResultValue id="publicLoginWrongPasswdValidationValue"
		dataValueID="publicLoginWrongPasswdVID" parent="baseERValue" />


	<!-- myWebElement definition -->
	<ate:myWebElement id="loginUserNameWE" parent="myBasePageModel" elementFind="loginUserNameEF" elementAction="loginUserNameEA">
	</ate:myWebElement>
	<ate:elementFindById id="loginUserNameEF"
		findByValue="email_login" />
	<ate:sendKeysAction id="loginUserNameEA" dataValue="userNameValue" parent="baseElementAction"/>
	<ate:stepInputDataValue id="userNameValue" dataValueID="1"
		parent="baseInputDataValue" />


	<!-- myWebElement definition -->
	<ate:myWebElement id="loginPasswordWE" parent="myBasePageModel" elementFind="loginPasswordEF" elementAction="loginPasswordEA">
	</ate:myWebElement>
	<ate:elementFindByXpath id="loginPasswordEF"
		findByValue="//input[@type='password']" />
	<ate:sendKeysAction id="loginPasswordEA" dataValue="passwordValue" parent="baseElementAction"/>
	<ate:stepInputDataValue id="passwordValue" dataValueID="3"
		parent="baseInputDataValue" />

	<!-- myWebElement definition -->
	<ate:myWebElement id="loginWrongPasswordWE" parent="myBasePageModel" elementFind="loginPasswordEF" elementAction="loginWrongPasswordEA">
	</ate:myWebElement>
	<ate:sendKeysAction id="loginWrongPasswordEA"
		dataValue="wrongPasswordValue" parent="baseElementAction"/>
	<ate:stepInputDataValue id="wrongPasswordValue"
		dataValueID="4" parent="baseInputDataValue" />

	<!-- step publicLogin_clickLoginbutton myWebElement definition -->
	<ate:myWebElement id="clickLoginButtonWE" parent="myBasePageModel" elementFind="loginButtonEF" elementAction="loginButtonEA">
	</ate:myWebElement>
	<ate:elementFindByXpath id="loginButtonEF"
		findByValue="//button[@type='submit']" />
	<ate:clickAction id="loginButtonEA" parent="baseElementAction"/>


</beans>