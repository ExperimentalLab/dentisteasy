<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:ate="http://www.bigtester.org/schema/ate" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd 
	http://www.springframework.org/schema/aop 
	http://www.springframework.org/schema/aop/spring-aop-4.0.xsd
	http://www.bigtester.org/schema/ate http://www.bigtester.org/schema/ate/ate.xsd">
	<!-- this registers beans annotated with @Aspect -->
	<aop:aspectj-autoproxy />

	<import resource="classpath:META-INF/logger-reporter.xml" />
	<import resource="classpath:META-INF/abstract_parent_bean2.xml" />

	<ate:homePage id="publicHomePage" homeUrl="http://www.dentisteasy.com/public"
		dataFile="dentistSmokeTest/pageObjects/publicHomePage.data.xml"
		parent="myAbstractPageObject" />

	<ate:myWebElement id="publicLoginLinkWE" parent="myBasePageModel">
		<constructor-arg ref="loginLinkEF" />
		<constructor-arg ref="loginLinkEA" />
	</ate:myWebElement>
	<ate:elementFindByXpath id="loginLinkEF"
		findByValue="//a[@href='/public/login']" />
	<ate:clickAction id="loginLinkEA" />

	<ate:myWebElement id="Clinic_C" parent="myBasePageModel">
		<constructor-arg ref="Clinic_C_EF" />
		<constructor-arg ref="Clinic_C_EA" />
	</ate:myWebElement>
	<ate:elementFindByXpath id="Clinic_C_EF"
		findByValue="//*[@title='Clinic C']/img[contains(@src,'map-marker-blue')]" />
	<ate:clickAction id="Clinic_C_EA" />

	<ate:myWebElement id="Service_Selector" parent="myBasePageModel">
		<constructor-arg ref="Service_Selector_EF" />
		<constructor-arg ref="Service_Selector_EA" />
	</ate:myWebElement>
	<ate:elementFindByXpath id="Service_Selector_EF"
		findByValue="//button[@data-id='srv_select']" />
	<ate:clickAction id="Service_Selector_EA" />

	<ate:myWebElement id="Service_Consultation" parent="myBasePageModel">
		<constructor-arg ref="Service_Consultation_EF" />
		<constructor-arg ref="Service_Consultation_EA" />
	</ate:myWebElement>
	<ate:elementFindByXpath id="Service_Consultation_EF"
		findByValue="//*[@id='panel-body']/div[1]/div/div/ul/li[1]/a" />
	<ate:clickAction id="Service_Consultation_EA" />

	<ate:myWebElement id="Location_Input" parent="myBasePageModel">
		<constructor-arg ref="Location_Input_EF" />
		<constructor-arg ref="Location_Input_EA" />
	</ate:myWebElement>
	<ate:elementFindById id="Location_Input_EF"
		findByValue="new_location_input" />
	<ate:sendKeysAction id="Location_Input_EA"
		dataValue="location_value" />
	<ate:stepDataValue id="location_value" dataValueID="5"
		parent="baseInputDataValue" />

	<ate:myWebElement id="Date_Selector" parent="myBasePageModel">
		<constructor-arg ref="Date_Selector_EF" />
		<constructor-arg ref="Date_Selector_EA" />
	</ate:myWebElement>
	<ate:elementFindById id="Date_Selector_EF"
		findByValue="dashboard-report-range" />
	<ate:clickAction id="Date_Selector_EA" />

	<ate:myWebElement id="Next_Week_Selection" parent="myBasePageModel">
		<constructor-arg ref="Next_Week_Selection_EF" />
		<constructor-arg ref="Next_Week_Selection_EA" />
	</ate:myWebElement>
	<ate:elementFindByXpath id="Next_Week_Selection_EF"
		findByValue="//li[contains(., 'Next week')]" />
	<ate:clickAction id="Next_Week_Selection_EA" />
	
	<ate:myWebElement id="Book_Button" parent="myBasePageModel">
		<constructor-arg ref="Book_Button_EF" />
		<constructor-arg ref="Book_Button_EA" />
	</ate:myWebElement>
	<ate:elementFindByXpath id="Book_Button_EF"
		findByValue="//a[contains(@href, '/public/practice?practiceId=')]" />
	<ate:clickAction id="Book_Button_EA" />
	
	<ate:myWebElement id="LocationList" parent="myBasePageModel">
		<constructor-arg ref="LocationList_EF" />
		<constructor-arg ref="LocationList_EA" />
	</ate:myWebElement>
	<ate:elementFindByXpath id="LocationList_EF"
		findByValue="//div[@class='pac-item'][1]" />
	<ate:clickAction id="LocationList_EA" />

	<ate:pagePropertyCorrectnessAsserter
		id="publicHomePagePropertyAsserter">
		<constructor-arg ref="publicHomePage" />
		<property name="stepERValue">
			<ref bean="publicHomePageValidationValue" />
		</property>
	</ate:pagePropertyCorrectnessAsserter>

	<ate:pageElementExistenceAsserter
		id="publicHomePageElementExistenceAsserter">
		<constructor-arg ref="publicHomePage" />
		<property name="stepERValue">
			<ref bean="publicHomePageValidationValue" />
		</property>
	</ate:pageElementExistenceAsserter>

	<ate:stepExpectedResultValue id="publicHomePageValidationValue"
		dataValueID="publicHomePageVID" parent="baseERValue" />


</beans>